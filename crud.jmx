              <stringProp name="script">import groovy.json.JsonBuilder
import groovy.json.JsonSlurper
import groovy.json.JsonOutput

def attrBuilder = new JsonBuilder()
def myAttr = attrBuilder {
	label &quot;att2&quot;
	static_value &apos;&apos;
	type &apos;dynamic&apos;
	value_type &apos;integer&apos;
}

def response = prev.getResponseDataAsString()
def json = new JsonSlurper().parseText(response)

// Assume we are updating just the first/only template
// &quot;response&quot; variable will be used in the UPDATE PUT request
json.templates[0].attrs.add(myAttr)
json.templates[0].data_attrs.add(myAttr)

vars.put(&quot;response&quot;, JsonOutput.toJson(json.templates[0]))
</stringProp>
